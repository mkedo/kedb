{
  "name": "mkedo/kedb",
  "license": "MIT",
  "description": "A database library with consistent interface, focused on extensibility and easier query decomposition",
  "require": {
    "php": ">=5.6"
  },
  "autoload": {
    "psr-4": {
      "Kedb\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Kedb\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "5.7.27",
    "squizlabs/php_codesniffer": "3.3"
  },
  "scripts": {
    "test": "phpunit",
    "cs": "phpcs --standard=PSR2 --encoding=utf-8 src/ tests/"
  }
}
